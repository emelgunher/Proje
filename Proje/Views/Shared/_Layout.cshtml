<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>@ViewBag.Title Emell </title>
    <!-- Bootstrap CSS -->
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" />
    <!-- animate CSS -->
    <link href="~/Content/css/animate.css" rel="stylesheet" />
    <!-- owl carousel CSS -->
    <link href="~/Content/css/owl.carousel.min.css" rel="stylesheet" />
    <!-- themify CSS -->
    <link href="~/Content/css/themify-icons.css" rel="stylesheet" />
    <!-- flaticon CSS -->
    <link href="~/Content/css/flaticon.css" rel="stylesheet" />
    <!-- magnific-popup CSS -->
    <link href="~/Content/css/magnific-popup.css" rel="stylesheet" />
    <!-- font awesome CSS -->
    <link href="~/Content/css/font-awesome.min.css" rel="stylesheet" />
    <!-- style CSS -->
    <link href="~/Content/css/style.css" rel="stylesheet" />
</head>
<body>

    <!--::menu part start::-->
    <header class="main_menu home_menu">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <nav class="navbar navbar-expand-lg navbar-light">
                        <a class="navbar-brand" href="/Home/Index">
                            <div class="section_tittle">
                                <h2> Emel Günher </h2>
                            </div>
                        </a>
                        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav"
                                aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                            <span class="navbar-toggler-icon"></span>
                        </button>
                        <div class="collapse navbar-collapse main-menu-item" id="navbarNav">
                            <ul class="navbar-nav">
                                <li class="nav-item">
                                    <a class="nav-link" href="/Home/Index">Hakkımda</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="/Home/Egitim">Eğitim</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="/Home/Sehir">Şehrim</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="/Home/Miras">Mirasımız</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="/Home/Iletisim">İletişim</a>
                                </li>
                                
                            </ul>
                        </div>
                    </nav>
                </div>
            </div>
        </div>
    </header>


    <section class="about_part section-padding">
        <div class="container body-content">
            @RenderBody()
        </div>
    </section>




    <footer class="footer_part  col-lg-12">
        <div class="row">
            <div class="col-lg-12">
                <div class="copyright_text text-center">
                    <P>
                        <script>document.write(new Date().getFullYear());</script> Web Teknolojileri  &hearts;   by Emel Günher
                        <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                    </P>
                </div>
            </div>
        </div>

    </footer>




    <!--::footer_part end::-->
    <!-- jquery plugins here-->
    <script src="~/Scripts/js/jquery-1.12.1.min.js"></script>
    <!-- popper js -->
    <script src="~/Scripts/js/popper.min.js"></script>
    <!-- bootstrap js -->
    <script src="~/Scripts/js/bootstrap.min.js"></script>
    <!-- magnific js -->
    <script src="~/Scripts/js/jquery.magnific-popup.min.js"></script>
    <!-- carousel js -->
    <script src="~/Scripts/js/owl.carousel.min.js"></script>
    <!-- easing js -->
    <script src="~/Scripts/js/jquery.easing.min.js"></script>
    <!--masonry js-->
    <script src="~/Scripts/js/masonry.pkgd.min.js"></script>
    <script src="~/Scripts/js/masonry.pkgd.js"></script>
    <!--form validation js-->
    <script src="~/Scripts/js/jquery.nice-select.min.js"></script>
    <script src="~/Scripts/js/contact.js"></script>
    <script src="~/Scripts/js/jquery.ajaxchimp.min.js"></script>
    <script src="~/Scripts/js/jquery.form.js"></script>
    <script src="~/Scripts/js/jquery.validate.min.js"></script>
    <script src="~/Scripts/js/mail-script.js"></script>
    <!-- counter js -->
    <script src="~/Scripts/js/jquery.counterup.min.js"></script>
    <script src="~/Scripts/js/waypoints.min.js"></script>
    <!-- custom js -->
    <script src="~/Scripts/js/custom.js"></script>


    @RenderSection("scripts", required: false)
</body>
</html>
